{
  "compileOnSave": false,
  "compilerOptions": {
    "noEmitOnError": false,
    "baseUrl": "build-web/src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "build-web/node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "exclude": [
    "build-ns",
    "build-web",
    "node_modules"
  ]
}
